{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-mbstring": "*",
        "ajcastro/eager-load-pivot-relations": "^0.2.2",
        "barryvdh/laravel-cors": "^3.0",
        "deployer/deployer": "^7.0",
        "encore/laravel-admin": "^1.8",
        "erjanmx/laravel-migrate-check": "^2.3",
        "fakerphp/faker": "^1.18",
        "fruitcake/laravel-cors": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.5.2",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "laravel-admin-ext/helpers": "1.*",
        "laravel-admin-ext/latlong": "1.x",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel/fortify": "^1.8",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "league/flysystem-sftp-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "^12.0",
        "psr/simple-cache": "^1.0",
        "psy/psysh": "0.11.2",
        "shvetsgroup/laravel-email-database-log": "^10.0",
        "spatie/laravel-backup": "^8.0",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.10",
        "doctrine/dbal": "^3.1",
        "laravel/dusk": "^6.21",
        "laravel/homestead": "^13.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "reliese/laravel": "^1.1",
        "shibuyakosuke/laravel-language-setting": "^1.2",
        "spatie/laravel-ignition": "^1.0",
        "staudenmeir/dusk-updater": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "exclude-from-classmap": [
            "vendor/laravel-admin-ext/latlong/src/Map/Google.php"
        ],
        "files":[
            "app/Vendor/laravel-admin-ext/latlong/src/Map/Google.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:model --nowrite",
            "@php artisan ide-helper:eloquent"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
