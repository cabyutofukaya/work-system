{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*",
        "ajcastro/eager-load-pivot-relations": "^0.2.2",
        "deployer/deployer": "^7.0",
        "encore/laravel-admin": "^1.8",
        "erjanmx/laravel-migrate-check": "^2.3",
        "fakerphp/faker": "^1.18",
        "fruitcake/laravel-cors": "^3.0",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.6.4",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "laravel-admin-ext/helpers": "1.*",
        "laravel-admin-ext/latlong": "1.x",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel/fortify": "^1.8",
        "laravel/framework": "^9.52",
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^4.12",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "*",
        "owen-it/laravel-auditing": "^13.0",
        "tightenco/ziggy": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.10",
        "doctrine/dbal": "^3.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:model --nowrite",
            "@php artisan ide-helper:eloquent"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
